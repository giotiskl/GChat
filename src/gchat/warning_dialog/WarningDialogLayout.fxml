<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>
<VBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gchat.warning_dialog.WarningDialogController">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <children>
                <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" scaleX="0.5" scaleY="0.5">
                    <image>
                        <Image url="@warn_icon.png" />
                    </image>
                </ImageView>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextFlow style="-fx-font-size: 14; -fx-font-weight: bold;" textAlignment="CENTER" />
               </children>
               <HBox.margin>
                  <Insets bottom="30.0" left="10.0" right="10.0" top="30.0" />
               </HBox.margin>
            </VBox>
            </children>
        </HBox>
        <Button fx:id="okButton" mnemonicParsing="false" onAction="#closeDialog" prefWidth="60.0" text="OK">
            <VBox.margin>
                <Insets bottom="10.0" right="10.0" />
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
